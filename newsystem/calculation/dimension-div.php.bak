    function checkTableExist($matTable) {
        $qrTbl = "SHOW TABLES LIKE '%$matTable%'";
        $objTbl = new SQL($qrTbl);
        $result = $objTbl->getResultOneRowArray();
        return $result;
    }

    function getThickList($matTable) {
        $qrData = "SELECT DISTINCT thickness FROM $matTable";
        $objData = new SQL($qrData);
        $result = $objData->getResultRowArray();
        return $result;
    }

    function getWidthList($matTable, $thickness) {
        $qrData = "SELECT DISTINCT width FROM $matTable WHERE thickness = $thickness";
        $objData = new SQL($qrData);
        $result = $objData->getResultRowArray();
        return $result;
    }

